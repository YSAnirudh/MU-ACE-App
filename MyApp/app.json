      "adaptiveIcon": {
        "foregroundImage": "./assets/rohai-main.png",
        "backgroundColor": "#121212"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A Discussion Forum and Availability Status App"
  }
}
